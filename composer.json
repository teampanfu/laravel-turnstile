{
    "name": "teampanfu/laravel-turnstile",
    "type": "library",
    "description": "Laravel package for Cloudflare's Turnstile CAPTCHA service",
    "keywords": ["laravel", "turnstile", "cloudflare", "captcha"],
    "license": "MIT",
    "authors": [
        {
            "name": "Robin",
            "email": "robin@panfu.me"
        }
    ],
    "require": {
        "php": "^8.2",
        "guzzlehttp/guzzle": "^7.8.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5.35|^11.3.6|^12.0.1"
    },
    "autoload": {
        "psr-4": {
            "Panfu\\Laravel\\Turnstile\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Panfu\\Laravel\\Turnstile\\Test\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Panfu\\Laravel\\Turnstile\\TurnstileServiceProvider"
            ],
            "aliases": {
                "Turnstile": "Panfu\\Laravel\\Turnstile\\Facades\\Turnstile"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
